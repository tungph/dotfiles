
# search for file to open
of() {
  [ $# -gt 0 ] && FILE="$(command ls -1F | fzy -q $@)" || FILE="$(command ls -1F | fzy)"

  [ "$FILE" != "" ] && open $FILE || echo " >> Cancelled"
    
}

